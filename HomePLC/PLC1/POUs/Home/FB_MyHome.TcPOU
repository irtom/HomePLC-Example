IDIGroup.AddInput(IO.FirstFloor.Entrance.wpDoor.fbButton2);
IDIGroup.AddInput(IO.FirstFloor.Entrance.wpDoor.fbButton3);
IDIGroup.AddInput(IO.FirstFloor.Entrance.wpDoor.fbButton4);
