    <Method Name="Configure" Id="{1b0e19df-2636-4e1f-a004-4c1e3c4c0259}" FolderPath="Methods\">
      <Declaration><![CDATA[// Configure
METHOD PROTECTED Configure]]></Declaration>
      <Implementation>
        <ST><![CDATA[CreateDIGroupAllOff(IDIGroup := fbDIGroupAllOff);
